aqua Hosts declares *

data SpellLocation:
    hostId: string
    spellId: string
    workerId: string

data Host:
    definition: string
    installationSpells: []SpellLocation
    relayId: string
    timestamp: string
    dummyDealId: string

data Hosts:
    decider: ?Host

func get() -> Hosts:
    <- Hosts(
        decider=nil
    )
