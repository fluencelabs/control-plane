aqua FailedStorage declares store_failed

import DealId from "../types.aqua"
import FAILED_DEALS from "../consts.aqua"
import list_push from "../fluence/spell.aqua"

-- What can fail in a way Decider itself can't fix?
-- * worker registration

data FailedDeal:
    deal_id: DealId
    message: string

func store_failed(deal_id: DealId, message: string):
    failed = FailedDeal(deal_id = deal_id, message = message)
    list_push("spell", FAILED_DEALS, failed)